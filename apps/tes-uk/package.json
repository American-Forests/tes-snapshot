{
  "name": "tes-uk",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently --names 'blitz,tegola' --prefix-colors 'bgBlue.bold,bgMagenta.bold' --kill-others 'yarn client:dev' 'yarn tegola'",
    "client:dev": "blitz prisma generate && blitz dev",
    "build": "blitz prisma generate && blitz build",
    "tegola": "dotenv -e .env.local -- tegola serve --config ./tegola/config.local.toml",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "blitz codegen && blitz prisma generate && eslint . --fix && tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "prepare": "husky install",
    "migrate": "blitz prisma migrate dev",
    "data:download": "aws s3 sync s3://dev-tes-uk-app-data ./local",
    "data:prepare": "cd sql && ./prepare.sh",
    "data:load": "cd sql && dotenv -e ../.env.local -- ./load.sh",
    "data": "yarn data:download && yarn data:prepare && yarn data:load",
    "cache:staging": "dotenv -e .env.staging -- ./tegola/cache.sh",
    "cache:prod": "dotenv -e .env.prod -- ./tegola/cache.sh"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "bash -c 'tsc --noEmit'"
    ]
  },
  "dependencies": {
    "@blitzjs/auth": "2.1.3",
    "@blitzjs/next": "2.1.3",
    "@blitzjs/rpc": "2.1.3",
    "@headlessui/react": "2.2.0",
    "@heroicons/react": "2.1.5",
    "@material-ui/core": "4.12.4",
    "@prisma/client": "5.21.1",
    "@radix-ui/react-dropdown-menu": "2.1.2",
    "@radix-ui/react-icons": "1.3.0",
    "@tailwindcss/forms": "0.5.6",
    "@tailwindcss/typography": "0.5.2",
    "@tippyjs/react": "4.2.6",
    "@turf/bbox": "6.5.0",
    "autoprefixer": "10.4.2",
    "betterknown": "1.0.3",
    "blitz": "2.1.3",
    "d3": "7.8.5",
    "formik": "2.4.6",
    "jotai": "2.4.3",
    "lodash": "4.17.21",
    "mapbox-gl": "2.15.0",
    "next": "15.0.2",
    "next-transpile-modules": "10.0.0",
    "postcss": "8.4.8",
    "prisma": "5.21.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "styled-components": "6.1.13",
    "tailwind-merge": "2.5.4",
    "tailwindcss": "3.4.14",
    "tsconfig": "*",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/d3": "7.4.3",
    "@types/lodash": "4.14.179",
    "@types/mapbox-gl": "^2.7.19",
    "@types/preview-email": "3.1.0",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "concurrently": "8.2.1",
    "dotenv-cli": "7.3.0",
    "eslint": "8.56.0",
    "eslint-config-next": "15.0.2",
    "husky": "^8.0.3",
    "jest": "27.0.1",
    "lint-staged": "15.2.0",
    "prettier": "2.8.8",
    "prettier-plugin-prisma": "5.0.0",
    "pretty-quick": "3.1.3",
    "preview-email": "3.1.0",
    "ts-jest": "27.0.1",
    "typescript": "~5.6.3"
  },
  "private": true,
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
