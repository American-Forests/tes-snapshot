{
  "name": "tes",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz prisma generate && blitz dev",
    "client:dev": "blitz prisma generate && blitz dev",
    "build": "blitz prisma generate && blitz build",
    "start": "blitz start",
    "start:production": "blitz start --port $PORT",
    "studio": "blitz prisma studio",
    "lint": "blitz codegen && blitz prisma generate && eslint . --fix && tsc --noEmit",
    "test": "blitz cg && jest --passWithNoTests",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "migrate": "blitz prisma migrate dev",
    "data:download": "aws s3 sync s3://tes-app-dev-geojson ./local",
    "download:national": "aws s3 sync s3://tes-app-dev-geojson/national ./local/national",
    "download:tesa": "aws s3 sync s3://tes-app-dev-geojson/location/$TESA ./local/location/$TESA",
    "download:itree": "aws s3 cp s3://dev-tes-app-data/itree.csv ./local/itree.csv",
    "data:validation": "node scripts/run-validation.js",
    "load:national": "bash -c 'cd aws/to_postgres && source .venv/bin/activate && dotenv -e ../../.env.local -- python to_postgres.py national ../../local/national'",
    "load:tesa": "if [ -z \"$TESA\" ]; then echo 'Error: TESA is not defined. Correct usage: \"TESA=boston yarn load:tesa\"' && exit 1; fi && cd aws/to_postgres && source .venv/bin/activate && dotenv -e ../../.env.local -- python to_postgres.py tesa ../../local/location/$TESA",
    "load:itree": "node load_itree.js",
    "pmtiles-config:tesa-dev-update": "aws s3 cp ./pmtiles/config/location/$TESA s3://tes-app-dev-geojson/location/$TESA --recursive",
    "pmtiles-config:national-dev-update": "aws s3 cp ./pmtiles/config/national s3://tes-app-dev-geojson/national --recursive"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "engines": {
    "yarn": "1.x",
    "node": ">=18.0.0 <19.0.0"
  },
  "dependencies": {
    "@blitzjs/auth": "2.1.3",
    "@blitzjs/next": "2.1.3",
    "@blitzjs/rpc": "2.1.3",
    "@headlessui/react": "2.2.0",
    "@heroicons/react": "2.1.5",
    "@honeycombio/opentelemetry-web": "0.3.0",
    "@mapbox/search-js-react": "^1.0.0",
    "@material-ui/core": "4.12.4",
    "@mdx-js/loader": "1.6.22",
    "@opentelemetry/auto-instrumentations-web": "0.39.0",
    "@prisma/client": "5.21.1",
    "@radix-ui/react-collapsible": "1.1.1",
    "@radix-ui/react-dialog": "1.1.2",
    "@radix-ui/react-dropdown-menu": "2.1.2",
    "@radix-ui/react-icons": "1.3.0",
    "@radix-ui/react-id": "1.1.0",
    "@radix-ui/react-popover": "1.1.2",
    "@radix-ui/react-portal": "1.1.2",
    "@radix-ui/react-select": "2.1.2",
    "@radix-ui/react-tabs": "1.1.1",
    "@reach/dialog": "0.16.2",
    "@reach/visually-hidden": "0.16.0",
    "@react-pdf/renderer": "4.0.0",
    "@sendgrid/mail": "7.7.0",
    "@stitches/react": "1.2.7",
    "@svgr/webpack": "6.2.1",
    "@tailwindcss/forms": "0.5.6",
    "@tailwindcss/typography": "0.5.2",
    "@tippyjs/react": "4.2.6",
    "@turf/bbox": "6.5.0",
    "autoprefixer": "10.4.2",
    "betterknown": "1.0.3",
    "blitz": "2.1.3",
    "classed-components": "2.0.0",
    "clsx": "1.1.1",
    "d3": "7.8.5",
    "d3-axis-for-react": "1.1.0",
    "envsafe": "2.0.3",
    "eslint-config-custom": "*",
    "file-saver": "2.0.5",
    "formik": "2.4.6",
    "geojson-extent": "0.3.2",
    "hashids": "2.2.10",
    "i18next": "^24.2.3",
    "i18next-http-backend": "^3.0.2",
    "jotai": "2.4.3",
